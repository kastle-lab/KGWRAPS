                    <ChemicalUsed>epoxy resin</ChemicalUsed>
                    <Time>
                        <value>1</value>
                        <unit>hours</unit>
                    </Time>
                </Mixing>
            </ChooseParameter>
            <ChooseParameter>
                <Molding>
                    <MoldingMode>hot-pressing</MoldingMode>
